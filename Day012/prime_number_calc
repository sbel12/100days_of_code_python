def is_prime(num):
    ch=-1
    for i in range(2,round(num**0.5+1)):
        print(i)
        if num%i==0.0:
            ch=0
            print(f'NOT a prime num, div by {i}')
            return 
        else:
            #print('it is a prime number')
            ch=1
    # if ch==0:
    #    print(f'NOT a prime num, div by {i}') 
    if ch==1:
        print('it is a prime number')
    else:
        print('wrong answer/input')
            
       
is_prime(73)
